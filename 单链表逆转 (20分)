#include <stdio.h>
#include <stdlib.h>

typedef int ElementType;
typedef struct Node* PtrToNode;
struct Node {
    ElementType Data;
    PtrToNode   Next;
};
typedef PtrToNode List;

List Read(); /* 细节在此不表 */
void Print(List L); /* 细节在此不表 */

List Reverse(List L);

int main()
{
    List L1, L2;
    L1 = Read();
    L2 = Reverse(L1);
    Print(L1);
    Print(L2);
    return 0;
}

List Reverse(List L) {
    PtrToNode Old_head = L, New_head = NULL, Temp;
    while (Old_head) {
        Temp = Old_head->Next;
        Old_head->Next = New_head;
        New_head = Old_head;
        Old_head = Temp;
    }
    L = New_head;
    return L;
}
